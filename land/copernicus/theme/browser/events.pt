<div tal:define="events view/events;" id="land-events">
  <h1>Events</h1>
  <p tal:content="events"></p>

  <!-- Events list -->
  <h4 class="land-title">Events</h4>
  <div class="listing block-inset">
    <div class="listing-body">
      <ul class="listing-items" tal:repeat="event events">
        <li class="listing-item">

          <div class="event snippet">
            <time datetime="" class="event-date">
              <span class="event-month">Sep</span>
              <span class="event-day">25</span>
              <span class="event-year">2016</span>
            </time>

            <div class="event-body">
              <h4 class="event-title">
                <a href="">Event title</a>
              </h4>
              <div class="event-description">
                <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Totam nobis dolores molestiae quo temporibus cupiditate exercitationem... <a href="#">Read more</a></p>
              </div>
            </div>

          </div>
        </li>
      </ul>

      <h4 class="text-right"><a href="./user-corner/events">view all events <i class="fa fa-arrow-circle-right"></i></a></h4>
    </div>
  </div><!-- Events list -->
</div>
